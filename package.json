{
    "name": "deuterium-boilerplate",
    "version": "1.0.0",
    "description": "Boilerplate using deuterium",
    "repository": "https://github.com/kbromma/deuterium-boilerplate.git",
    "author": "Kyle Bromma <kbromma@gmail.com>",
    "license": "MIT",
    "scripts": {
        "dev:server": "nodemon --ignore lib --exec babel-node src/_server",
        "dev:client": "webpack-dev-server --progress",
        "lint": "eslint src && flow",
        "test": "cross-env NODE_ENV=production node lib/_server",
        "build": "babel-node build.js",
        "start": "cross-env NODE_ENV=production pm2 start lib/_server/index.js && pm2 logs",
        "stop": "pm2 delete index",
        "list-pm2": "pm2 list"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.6",
        "@babel/node": "^7.0.0",
        "@babel/plugin-proposal-do-expressions": "^7.0.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
        "@babel/plugin-proposal-optional-chaining": "^7.0.0",
        "@babel/plugin-proposal-pipeline-operator": "^7.3.0",
        "@babel/plugin-transform-runtime": "^7.1.0",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-flow": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "@babel/runtime-corejs2": "^7.3.1",
        "@deuterium/babel-preset": "^1.3.0",
        "@deuterium/eslint-config": "^1.3.0",
        "@deuterium/jest-config": "^1.2.5",
        "@deuterium/prettier-config": "^1.0.4",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.0.0",
        "babel-plugin-flow-react-proptypes": "^25.0.0",
        "babel-preset-flow": "^6.23.0",
        "eslint": "^5.12.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.0.0",
        "eslint-plugin-flowtype": "^3.2.1",
        "eslint-plugin-import": "^2.15.0",
        "eslint-plugin-jsx-a11y": "^6.2.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "flow-bin": "^0.91.0",
        "jest": "^24.0.0",
        "nodemon": "^1.18.9",
        "prettier": "^1.16.1",
        "prop-types": "^15.6.2"
    },
    "dependencies": {
        "@babel/polyfill": "^7.2.5",
        "@babel/runtime": "^7.3.1",
        "@deuterium/env": "^1.2.1",
        "@deuterium/koa": "^1.3.2",
        "@deuterium/util": "^1.3.0",
        "babel-loader": "^8.0.5",
        "cross-env": "^5.2.0",
        "koa": "^2.7.0",
        "koa-helmet": "^4.0.0",
        "koa-mount": "^4.0.0",
        "koa-router": "^7.4.0",
        "koa-sslify": "^4.0.3",
        "koa-static": "^5.0.0",
        "pm2": "^3.2.9",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-hot-loader": "^4.6.5",
        "shelljs": "^0.8.3",
        "webpack": "^4.29.0",
        "webpack-cli": "^3.2.1",
        "webpack-dev-server": "^3.1.14"
    }
}
